    <h2>Написати резюме</h2>
    <div>
        <?php echo link_to_route('resumes', 'Мої резюме'); ?> <!-- Створення силки -->
    </div>
<?php $__env->stopSection(); ?>

<?php $__env->startSection('content'); ?>
    <div class="row header-resume">
        <h3 class="formTitle header-text-resume"><b><?php echo e(trans('content.addresume')); ?></b></h3>
    </div>
    <?php echo Form::open([
        'route' => 'resume.store',
        'enctype' => 'multipart/form-data',
        'id'=>'form_id'
    ]); ?>

        <?php echo $__env->make('Resume._form.create', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>
    <?php echo Form::close(); ?>

<?php $__env->stopSection(); ?>
<script src="https//code.jquery.com/jquery-1.11.2.min.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
        $( ".form-control" ).change(function() {
            $('#form_id').submit(function () {
                $(window).unbind('beforeunload');
            });
        });
        $(".js-example-basic-multiple").select2();

        $("#selectIndustry").select2();

        $("#selectCurrency").select2();

    });
</script>
<?php echo $__env->make('app', array_except(get_defined_vars(), array('__data', '__path')))->render(); ?>